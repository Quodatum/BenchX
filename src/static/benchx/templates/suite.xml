<div>
	<div class="row">
	   <h4>
		<a href="#/suite" class="btn btn-default btn-sm" title="Suites">
			suites
		</a>
		/{{suite.name}}
		</h4>
		<div class="btn-group btn-group-sm">
			<a href="{{suite.library}}" class="btn btn-default btn-sm" title="library">
				<i class="glyphicon glyphicon-book"></i>
				Library
			</a>
			<a href="{{suite.session}}" class="btn btn-default btn-sm" title="Session">
				<i class="glyphicon glyphicon-cog"></i>
				Run
			</a>
		</div>

	</div>
	<div class="row">
		<ul ng-repeat="item in suite.queries">
			<li title="{{item.src}}">{{item.name}}
			<pre>{{item.src}}</pre></li>
		</ul>
	</div>
</div>