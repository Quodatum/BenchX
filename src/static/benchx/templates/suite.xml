<div>
	<div class="row form-group" role="navigation">
		<ol class="breadcrumb navbar-left">
			<li>
				<a href="#/suite" title="Suites">
					suites
				</a>
			</li>

			<li>
				{{suite.name}}
			</li>

			<li>
				<div class="btn-group  btn-group-sm navbar-btn">
					<a href="{{suite.library}}" class="btn btn-default btn-sm" title="library">
						<i class="glyphicon glyphicon-book"></i>
						Library
					</a>
					<a href="{{suite.session}}" class="btn btn-default btn-sm" title="Session">
						<i class="glyphicon glyphicon-cog"></i>
						Run
					</a>
				</div>
			</li>
		</ol>

	</div>

	<div class="row">
		<ul ng-repeat="item in suite.queries">
			<li title="{{item.src}}">
				{{item.name}}
				<pre>{{item.src}}</pre>
			</li>
		</ul>
	</div>
