<div ng-swipe-right="swipe()">
	<p>Stored records:</p>
	<table class="table table-condensed table-striped table-bordered">
		<thead>
			<th>Title</th>
			<th>Suite</th>
			<th>Created</th>
		</thead>
		<tbody>
			<tr ng-repeat="doc in docs">
				<td>
					<a href="#/library/{{doc.id}}">{{doc.description}}</a>
				</td>
				<td>{{doc.suite}}</td>
				<td>{{doc.created}}</td>
			</tr>
		</tbody>
	</table>
</div>