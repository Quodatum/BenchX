<div class="row">

	<div class="panel  panel-info panel-body col-md-4">
		<div class="panel-heading">
			<h3 class="panel-title">Java environment values</h3>
		</div>
		<div class="panel-body table-responsive">
			<table class="table table-striped">
				<thead>
					<tr>
						<th>Name</th>
						<th>Value</th>
					</tr>
				</thead>
				<tbody>
					<tr ng-repeat="e in envs">
						<td>{{e.name}}</td>
						<td>{{e.value}}</td>
					</tr>
				</tbody>
			</table>
		</div>
	</div>
	<div class="col-md-4">
SessionId: <span class="label label-default">{{state.session}}</span>
</div>
</div>